{
  "name": "fec-reservation-calendar",
  "version": "1.0.0",
  "description": "reservation calendar for an airbnb clone item detail page",
  "main": "index.js",
  "scripts": {
    "react-dev": "webpack -d --watch",
    "start": "nodemon server/app.js",
    "seed": "node seed.js",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/spec/mock/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/dist/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/client/src/stylesheets/"
    ],
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^25.2.2",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "react-test-renderer": "^16.14.0",
    "supertest": "^6.0.1",
    "webpack": "^2.2.1",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@testing-library/react": "^11.1.2",
    "axios": "^0.21.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "1.19.0",
    "express": "^4.15.0",
    "mongoose": "^5.10.11",
    "morgan": "1.9.1",
    "node": "^12.18.0",
    "nodemon": "^1.18.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
